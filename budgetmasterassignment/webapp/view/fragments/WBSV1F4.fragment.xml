<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core"
>
    <SelectDialog
        id="idSDWBSV1F4"
        title="Budget Code"
        noDataText="No Data"
        search="onValueHelpSearch_WBSV1"
        liveChange="onValueHelpSearch_WBSV1"
        confirm="onValueHelpConfirm_WBSV1"
        cancel="onValueHelpCancel_WBSV1"
        contentWidth="250px"
        multiSelect="true"
        items="{/ZpsBudWbsSet}"
        showClearButton="true"
    >
        <items>
            <StandardListItem
                id="idSLWBSV1ValueHelp"
                title="{Posid}"
                description="{Post1}"
                wrapping="true"
            />
        </items>
    </SelectDialog>
</core:FragmentDefinition>
